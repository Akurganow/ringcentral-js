{
  "name": "@ringcentral/sdk",
  "version": "4.0.0-alpha.0",
  "scripts": {
    "clean": "rimraf dist/* lib/* es6/* coverage/* .nyc_output .rpt2_cache",
    "test": "npm run mocha",
    "mocha": "mocha --opts mocha.opts",
    "mocha:watch": "npm run mocha -- --watch",
    "karma": "karma start karma.conf.js",
    "nyc": "nyc mocha --opts mocha.opts",
    "prepublishOnly": "npm run build && npm run nyc && npm run karma",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "npm run clean && npm run build:tsc:es5 && npm run build:tsc:es6 && npm run build:webpack",
    "build:tsc:es5": "tsc",
    "build:tsc:es6": "tsc --project tsconfig.es6.json",
    "build:webpack": "webpack",
    "start": "npm-run-all -p watch:tsc:es5 watch:tsc:es6 watch:webpack",
    "watch:tsc:es5": "npm run build:tsc:es5 -- --watch --preserveWatchOutput",
    "watch:tsc:es6": "npm run build:tsc:es6 -- --watch --preserveWatchOutput",
    "watch:webpack": "npm run build:webpack -- --watch"
  },
  "dependencies": {
    "dom-storage": "2.1.0",
    "is-plain-object": "2.0.4",
    "node-fetch": "2.2.1"
  },
  "devDependencies": {
    "@ringcentral/sdk-utils": "^4.0.0-alpha.0",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.0",
    "coveralls": "3.0.2",
    "karma": "3.1.1",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "13.1.0",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.9",
    "ts-loader": "5.2.2",
    "ts-node": "7.0.1",
    "tslib": "1.9.3",
    "typescript": "3.1.3",
    "webpack": "4.23.0",
    "webpack-cli": "3.1.2"
  },
  "main": "./lib/index.js",
  "module": "./es6/index.js",
  "types": "./lib/SDK.d.ts",
  "browser": "./dist/ringcentral.js",
  "author": {
    "name": "RingCentral, Inc.",
    "email": "devsupport@ringcentral.com"
  },
  "contributors": [
    {
      "name": "Kirill Konshin"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ringcentral/ringcentral-js.git"
  },
  "bugs": {
    "url": "https://github.com/ringcentral/ringcentral-js/issues"
  },
  "homepage": "https://github.com/ringcentral/ringcentral-js",
  "engines": {
    "node": ">=4"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
