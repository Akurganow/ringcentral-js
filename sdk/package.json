{
  "name": "@ringcentral/sdk",
  "version": "4.0.0-beta.1",
  "scripts": {
    "clean": "rimraf build/* lib/* coverage/*",
    "gulp": "gulp",
    "build": "npm run clean && npm run gulp",
    "test": "npm run nyc && npm run karma",
    "mocha": "mocha --opts mocha.opts",
    "mocha:watch": "npm run mocha -- --watch",
    "karma": "karma start karma.conf.js",
    "karma:watch": "npm run karma -- --no-single-run --auto-watch",
    "nyc": "nyc mocha --opts mocha.opts",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "is-plain-object": "2.0.4"
  },
  "devDependencies": {
    "@ringcentral/sdk-utils": "0.0.0",
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.6",
    "@types/sinon": "4.3.0",
    "coveralls": "3.0.0",
    "gulp": "3.9.1",
    "karma": "2.0.0",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.3",
    "ts-node": "5.0.0",
    "tslib": "1.9.0",
    "typescript": "2.7.2"
  },
  "preferGlobal": false,
  "private": false,
  "main": "./build/SDK.js",
  "types": "./build/SDK.d.ts",
  "browser": "./build/ringcentral.js",
  "author": {
    "name": "RingCentral, Inc.",
