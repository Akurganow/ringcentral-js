{
  "name": "rcsdk",
  "version": "1.3.0",
  "scripts": {
    "start": "node server.js",
    "build": "./node_modules/.bin/gulp",
    "watch": "npm run build -- watch",
    "watch-all": "npm run build -- watch-all",
    "test": "./node_modules/.bin/bower install && npm run build && npm run mocha && npm run karma",
    "test-api": "./node_modules/.bin/mocha --recursive ./test/specs-api/**/*.js",
    "mocha": "./node_modules/.bin/mocha --recursive ./lib/**/*.js",
    "karma": "./node_modules/karma/bin/karma start ./test-utils/karma.conf.js",
    "karma-chrome": "npm run karma -- --browsers Chrome",
    "karma-watch": "npm run karma-chrome -- --no-single-run --auto-watch --log-level warn",
    "istanbul": "./node_modules/.bin/istanbul cover -x *-spec.js --dir=./build/coverage --report=lcov --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --reporter spec lib/**/*-spec.js"
  },
  "dependencies": {
    "crypto-js": "*",
    "dom-storage": "*",
    "es6-promise": "*",
    "pubnub": "*",
    "xhr2": "*"
  },
  "devDependencies": {
    "chai": "*",
    "express": "*",
    "gulp": "*",
    "gulp-jshint": "*",
    "gulp-rename": "*",
    "gulp-replace": "*",
    "gulp-sourcemaps": "*",
    "gulp-typescript": "^2.6.0",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "gulp-webpack-build": "*",
    "gulp-wrap-js": "*",
    "istanbul": "*",
    "karma": "*",
    "karma-chai-plugins": "*",
    "karma-coverage": "*",
    "karma-html-reporter": "*",
    "karma-mocha": "*",
    "karma-mocha-reporter": "*",
    "karma-phantomjs-launcher": "*",
    "karma-requirejs": "*",
    "mocha": "*",
    "sinon": "*",
    "sinon-chai": "*",
    "soap": "*",
    "ts-loader": "*",
    "typescript": "*",
    "webpack": "*"
  },
  "preferGlobal": false,
  "private": false,
  "main": "./lib/RCSDK.js",
  "author": {
    "name": "RingCentral, Inc.",
    "email": "devsupport@ringcentral.com"
  },
  "contributors": [
    {
      "name": "Kirill Konshin"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ringcentral/js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ringcentral/js-sdk/issues"
  },
  "homepage": "https://github.com/ringcentral/js-sdk",
  "engines": {
    "node": ">=0.10.36"
  },
  "license": "MIT"
}
