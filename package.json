{
  "name": "@ringcentral/mono",
  "version": "4.0.0",
  "scripts": {
    "postinstall": "lerna bootstrap",
    "clean": "lerna clean && lerna run clean && rm -rf node_modules",
    "start": "lerna run start --parallel",
    "build": "lerna run build",
    "test": "lerna run test",
    "coveralls": "lerna run coveralls",
    "publish:canary": "lerna publish --canary --force-publish=\"*\"",
    "publish:fromgit": "lerna publish from-git",
    "prepare:release": "lerna version --tag-version-prefix=\"\" --force-publish=\"*\"",
    "prepare:prerelease": "npm run prepare:release -- prerelease --preid alpha",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix",
    "lint:all": "npm run lint './*/src/**/*.ts*'",
    "lint:staged": "lint-staged"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringcentral/ringcentral-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ringcentral/ringcentral-js/issues"
  },
  "homepage": "https://github.com/ringcentral/ringcentral-js#readme",
  "devDependencies": {
    "eslint": "5.8.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-typescript": "1.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-typescript": "0.13.0",
    "husky": "1.1.3",
    "lint-staged": "8.0.4",
    "lerna": "3.4.3",
    "prettier": "1.15.1",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "20.1.1"
  }
}
