{
  "name": "@ringcentral/react",
  "version": "4.0.0-alpha.0",
  "scripts": {
    "clean": "rimraf lib/* es6/* .cache",
    "build": "npm run clean && npm run build:tsc:es5 && npm run build:tsc:es6",
    "build:tsc:es5": "tsc",
    "build:tsc:es6": "tsc --project tsconfig.es6.json",
    "start": "npm run watch:tsc",
    "watch:tsc": "npm run build:tsc -- --watch --preserveWatchOutput",
    "demo": "parcel demo/index.html -p 8080",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.1.5",
    "@babel/polyfill": "7.0.0",
    "@babel/runtime": "7.1.5",
    "@ringcentral/sdk": "^4.0.0-alpha.0",
    "@ringcentral/sdk-utils": "^4.0.0-alpha.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "4.3.1",
    "parcel-bundler": "1.10.3",
    "react": "16.6.0",
    "react-dom": "16.6.1",
    "react-router-dom": "4.3.1",
    "rimraf": "2.6.2",
    "ts-loader": "5.2.2",
    "tslib": "1.9.3",
    "typescript": "3.1.3",
    "webpack": "4.23.0",
    "webpack-cli": "3.1.2"
  },
  "peerDependencies": {
    "@ringcentral/sdk": ">=4",
    "react": "*",
    "react-dom": "*"
  },
  "main": "./lib/index.js",
  "module": "./es6/index.js",
  "types": "./lib/index.d.ts",
  "author": {
    "name": "RingCentral, Inc.",
    "email": "devsupport@ringcentral.com"
  },
  "contributors": [
    {
      "name": "Kirill Konshin"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ringcentral/ringcentral-js.git"
  },
  "bugs": {
    "url": "https://github.com/ringcentral/ringcentral-js/issues"
  },
  "homepage": "https://github.com/ringcentral/ringcentral-js",
  "engines": {
    "node": ">=4"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
